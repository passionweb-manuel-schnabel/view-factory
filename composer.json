{
  "name": "passionweb/viewfactory",
  "description": "Shows the usage of the ViewFactory which was introduced in TYPO3 v13",
  "type": "typo3-cms-extension",
  "minimum-stability": "stable",
  "version": "1.0.0",
  "homepage": "https://www.passionweb.de",
  "authors": [
    {
      "name": "Manuel Schnabel",
      "role": "Developer"
    }
  ],
  "require": {
    "typo3/cms-core": "^13.4"
  },
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Passionweb\\ViewFactory\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "viewfactory"
    }
  }
}